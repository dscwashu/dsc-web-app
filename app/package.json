    "clsx": "^1.1.1",
    "cypress": "^4.7.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "firebase": "^7.15.0",
    "husky": "^4.2.5",
    "install": "^0.13.0",
